{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "allowJs": false,
        "skipLibCheck": true,
        "esModuleInterop": false,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "typeRoots": [
            "../node_modules/@types",
            "../@types"
        ],
        "paths": {
            "@Pages/*": [
                "./src/Pages/*"
            ],
            "@Components/*": [
                "./src/Components/*"
            ],
            "@logs/*": [
                "./src/logs/*"
            ],
            "@/*": [
                "./src/*"
            ],
            "@Utils/*": [
                "./src/Utils/*"
            ],
            "@Hooks/*": [
                "./src/Hooks/*"
            ]
        }
    },
    "include": [
        "src"
    ],
    "references": [
        {
            "path": "./tsconfig.node.json"
        }
    ]
}